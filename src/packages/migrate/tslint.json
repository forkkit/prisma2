{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "interface-name": null,
    "object-literal-sort-keys": null,
    "max-classes-per-file": null,
